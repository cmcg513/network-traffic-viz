function dummyonunload(evt)
{
	if(oldonloadfunction)
	{
		oldonloadfunction(evt);
	}
}
window.onunload=dummyonunload;

var oldshowpagefunction = window.onshowpage;
function cnnshowpageSend(evt)
{

        if(evt.persisted)
	{
		cnnSend();
	}
	oldshowpagefunction(evt);
}
