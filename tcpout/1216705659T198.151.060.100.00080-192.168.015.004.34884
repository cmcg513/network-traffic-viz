function closeFlyout() {
	//log.debug("Close flyout.");
	if(typeof botFloatFlyoutObject != 'undefined' && botFloatFlyoutObject.isflyoutOpen) {
		botFloatFlyoutObject.closeflyOutMover();
	}
}

